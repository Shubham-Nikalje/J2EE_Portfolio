<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 	
	xsi:schemaLocation=" http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"> 
	
<bean class="com.jspider.springcoremanytomany.beans.CarBean" name="car1">
<property name="car_no" value="0028" />
<property name="car_company"  value="Tata" />
<property name="drivers" >
<list>
<bean class="com.jspider.springcoremanytomany.beans.DriverBean" name="driver1">
<property name="id" value="1"  />
<property name="name" value="shubham" />
</bean>
<bean class="com.jspider.springcoremanytomany.beans.DriverBean" name="driver2">
<property name="id" value="2"  />
<property name="name" value="vivek" />
</bean>
</list>
</property>
</bean>	

